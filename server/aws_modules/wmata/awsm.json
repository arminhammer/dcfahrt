{
  "name": "wmata",
  "version": "0.0.1",
  "location": "https://github.com/...",
  "author": "",
  "description": "",
  "resources": {
    "cloudFormation": {
      "LambdaIamPolicyDocumentStatements": [
        {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Sid": "Stmt1443965090000",
              "Effect": "Allow",
              "Action": [
                "s3:PutObject"
              ],
              "Resource": [
                "arn:aws:s3:::cache.dcfahrt.com/*"
              ]
            }
          ]
        }
      ],
      "ApiGatewayIamPolicyDocumentStatements": [],
      "Resources": {
        "DCFahrtCacheBucket": {
          "Type": "AWS::S3::Bucket",
          "Properties": {
            "AccessControl": "Private",
            "BucketName": {
              "Ref": "CacheBucket"
            }
          },
          "DeletionPolicy": "Retain"
        }
      }
    }
  }
}